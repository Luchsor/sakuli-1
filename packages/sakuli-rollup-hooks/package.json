{
  "name": "@sakuli/rollup-hooks",
  "version": "1.0.0",
  "description": "provide test script preprocessing with rollup",
  "keywords": [
    "rollup",
    "sakuli",
    "testing",
    "e2e"
  ],
  "author": "Tim Keiner <keiner.tim@gmail.com>",
  "homepage": "https://github.com/sakuli/sakuli#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sakuli/sakuli.git"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "build": "tsc",
    "typedoc": "typedoc --out ../../docs/api/sakuli-core --hideGenerator --mode file --excludeExternals --readme README.md --exclude *.spec.* src/",
    "test:ci": "jest --runInBand --no-cache --coverage --passWithNoTests"
  },
  "bugs": {
    "url": "https://github.com/sakuli/sakuli/issues"
  },
  "devDependencies": {
    "@types/common-tags": "^1.4.0",
    "@types/jest": "^24.0.11",
    "@types/mock-fs": "^3.6.30",
    "@types/node": "^11.13.0",
    "@types/rollup": "^0.54.0",
    "@types/rollup-plugin-node-resolve": "^4.0.0",
    "jest": "^23.6.0",
    "mock-fs": "^4.8.0",
    "rollup-plugin-typescript2": "^0.19.3",
    "sneer": "^1.0.1",
    "ts-jest": "^23.10.4",
    "typedoc": "^0.13.0",
    "typescript": "^3.4.1"
  },
  "dependencies": {
    "@sakuli/commons": "1.0.0",
    "@sakuli/core": "1.0.0",
    "rollup-plugin-node-resolve": "^4.0.1"
  }
}
